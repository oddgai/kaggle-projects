   "source": [
    "## 特徴量重要度分析"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 特徴量重要度の集計\n",
    "importance_agg = feature_importance.groupby('feature')['importance'].agg(['mean', 'std']).reset_index()\n",
    "importance_agg = importance_agg.sort_values('mean', ascending=False)\n",
    "\n",
    "print(\"=== 特徴量重要度 Top 20 ===\")\n",
    "for i, row in importance_agg.head(20).iterrows():\n",
    "    print(f\"{row['feature']:25s}: {row['mean']:8.1f} ± {row['std']:6.1f}\")\n",
    "\n",
    "# 特徴量重要度の可視化\n",
    "plt.figure(figsize=(12, 8))\n",
    "top_features = importance_agg.head(20)\n",
    "plt.barh(range(len(top_features)), top_features['mean'])\n",
    "plt.yticks(range(len(top_features)), top_features['feature'])\n",
    "plt.xlabel('重要度')\n",
    "plt.title('特徴量重要度 Top 20')\n",
    "plt.gca().invert_yaxis()\n",
    "plt.tight_layout()\n",
    "plt.show()\n",
    "\n",
    "# 新特徴量の効果分析\n",
    "print(\"\\n=== 新特徴量の効果分析 ===\")\n",
    "new_feature_categories = {\n",
    "    '名前関連': ['Title_Grouped', 'Name_Length', 'Surname_Count'],\n",
    "    'チケット関連': ['Ticket_Length', 'Ticket_IsNumeric', 'Ticket_Prefix', 'Ticket_Count', 'Ticket_First_Digit'],\n",
    "    '客室関連': ['Cabin_Deck', 'Cabin_Number', 'Cabin_Count', 'Cabin_Number_Odd'],\n",
    "    '家族構成詳細': ['IsSmallFamily', 'IsLargeFamily', 'HasSpouse', 'HasChildren', 'Spouse_Children', 'ParentsChildren_Alone'],\n",
    "    '非線形・統計': ['Age_Log', 'Age_Squared', 'Fare_Log', 'Fare_Sqrt', 'Age_Rank_SexPclass', 'Fare_Rank_Pclass', 'Age_Rank_Family'],\n",
    "    '交互作用': ['Age_Fare_Interaction', 'Age_FamilySize', 'Fare_FamilySize', 'Pclass_FamilySize']\n",
    "}\n",
    "\n",
    "for category, features in new_feature_categories.items():\n",
    "    category_importance = importance_agg[importance_agg['feature'].isin(features)]\n",
    "    if not category_importance.empty:\n",
    "        total_importance = category_importance['mean'].sum()\n",
    "        print(f\"{category}: {total_importance:.1f} (特徴量数: {len(category_importance)})\")\n",
    "        for _, row in category_importance.head(3).iterrows():\n",
