   "source": [
    "# exp004 - 高度な特徴量エンジニアリング\n",
    "\n",
    "exp001-003の結果を踏まえ、名前、チケット、客室等からより高度な特徴量を抽出してモデル性能向上を目指す"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "import numpy as np\n",
    "import lightgbm as lgb\n",
    "from sklearn.model_selection import StratifiedKFold\n",
    "from sklearn.metrics import accuracy_score\n",
    "from sklearn.preprocessing import LabelEncoder\n",
    "import matplotlib.pyplot as plt\n",
    "import seaborn as sns\n",
    "import japanize_matplotlib\n",
    "import warnings\n",
    "warnings.filterwarnings('ignore')\n",
    "\n",
    "plt.rcParams['font.family'] = 'IPAexGothic'\n",
    "\n",
    "# データ読み込み\n",
    "train_df = pd.read_csv('/Users/koki.ogai/Documents/ghq/github.com/oddgai/kaggle-projects/titanic/data/train.csv')\n",
    "test_df = pd.read_csv('/Users/koki.ogai/Documents/ghq/github.com/oddgai/kaggle-projects/titanic/data/test.csv')\n",
    "\n",
    "print(f\"Train shape: {train_df.shape}\")\n",
    "print(f\"Test shape: {test_df.shape}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 高度な特徴量エンジニアリング"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "def advanced_feature_engineering(df, is_train=True):\n",
    "    \"\"\"\n",
    "    高度な特徴量エンジニアリング\n",
    "    \"\"\"\n",
    "    # コピーを作成\n",
    "    df = df.copy()\n",
    "    \n",
    "    # ========== 1. 名前からの特徴量 ==========\n",
    "    # 敬称抽出\n",
    "    df['Title'] = df['Name'].str.extract(r' ([A-Za-z]+)\\.')\n",
    "    \n",
    "    # 敬称のグループ化（詳細版）\n",
    "    title_mapping = {\n",
    "        'Mr': 'Mr',\n",
    "        'Mrs': 'Mrs', \n",
    "        'Miss': 'Miss',\n",
    "        'Master': 'Master',\n",
    "        'Dr': 'Rare_High',  # 医師（高地位）\n",
    "        'Rev': 'Rare_High', # 牧師（高地位）\n",
    "        'Col': 'Rare_High', # 大佐（高地位）\n",
    "        'Major': 'Rare_High', # 少佐（高地位）\n",
    "        'Capt': 'Rare_High', # 大尉（高地位）\n",
    "        'Countess': 'Rare_Noble', # 伯爵夫人（貴族）\n",
    "        'Lady': 'Rare_Noble',    # 婦人（貴族）\n",
    "        'Sir': 'Rare_Noble',     # 卿（貴族）\n",
    "        'Don': 'Rare_Noble',     # ドン（貴族）\n",
    "        'Dona': 'Rare_Noble',    # ドナ（貴族）\n",
    "        'Jonkheer': 'Rare_Noble', # オランダ貴族\n",
    "        'Mlle': 'Miss',  # フランス語Miss\n",
    "        'Mme': 'Mrs',    # フランス語Mrs\n",
    "        'Ms': 'Mrs'      # 現代的Mrs\n",
    "    }\n",
    "    df['Title_Grouped'] = df['Title'].map(title_mapping)\n",
    "    df['Title_Grouped'] = df['Title_Grouped'].fillna('Other')\n",
    "    \n",
    "    # 名前の長さ（社会的地位の指標）\n",
    "    df['Name_Length'] = df['Name'].str.len()\n",
    "    \n",
    "    # 苗字抽出と家族グループサイズ\n",
    "    df['Surname'] = df['Name'].str.split(',').str[0]\n",
    "    # 全データで苗字をカウント（train+testで計算）\n",
    "    if is_train:\n",
    "        # 訓練時は全データ（train+test）で苗字カウントを計算\n",
    "        all_surnames = pd.concat([train_df['Name'], test_df['Name']]).str.split(',').str[0]\n",
    "        surname_counts = all_surnames.value_counts()\n",
    "    else:\n",
    "        # テスト時は保存された値を使用（実装時に調整）\n",
    "        all_surnames = pd.concat([train_df['Name'], test_df['Name']]).str.split(',').str[0]\n",
    "        surname_counts = all_surnames.value_counts()\n",
    "    \n",
    "    df['Surname_Count'] = df['Surname'].map(surname_counts)\n",
    "    \n",
    "    # ========== 2. チケットからの特徴量 ==========\n",
    "    # チケットの長さ\n",
    "    df['Ticket_Length'] = df['Ticket'].str.len()\n",
    "    \n",
    "    # チケットが数字のみかどうか\n",
    "    df['Ticket_IsNumeric'] = df['Ticket'].str.isnumeric().astype(int)\n",
    "    \n",
    "    # チケットプレフィックス（英字部分）\n",
    "    df['Ticket_Prefix'] = df['Ticket'].str.extract(r'^([A-Za-z]+/?)').fillna('NUMERIC')\n",
    "    df['Ticket_Prefix'] = df['Ticket_Prefix'].str.replace('/', '')\n",
    "    \n",
    "    # 同一チケット人数（グループチケット）\n",
    "    if is_train:\n",
    "        all_tickets = pd.concat([train_df['Ticket'], test_df['Ticket']])\n",
    "        ticket_counts = all_tickets.value_counts()\n",
    "    else:\n",
    "        all_tickets = pd.concat([train_df['Ticket'], test_df['Ticket']])\n",
    "        ticket_counts = all_tickets.value_counts()\n",
    "    \n",
    "    df['Ticket_Count'] = df['Ticket'].map(ticket_counts)\n",
    "    \n",
    "    # チケット番号の最初の数字\n",
    "    df['Ticket_First_Digit'] = df['Ticket'].str.extract(r'(\\d)').astype(float)\n",
    "    \n",
    "    # ========== 3. 客室からの特徴量 ==========\n",
    "    # 客室情報の有無\n",
    "    df['HasCabin'] = (~df['Cabin'].isnull()).astype(int)\n",
    "    \n",
    "    # 客室デッキ（最初のアルファベット）\n",
    "    df['Cabin_Deck'] = df['Cabin'].str.extract(r'^([A-Za-z])').fillna('Unknown')\n",
    "    \n",
    "    # 客室番号（数字部分）\n",
    "    df['Cabin_Number'] = df['Cabin'].str.extract(r'(\\d+)').astype(float)\n",
    "    \n",
    "    # 保有客室数（スペース区切りで複数客室を持つ場合）\n",
    "    df['Cabin_Count'] = df['Cabin'].fillna('').str.split().str.len()\n",
    "    df.loc[df['Cabin'].isnull(), 'Cabin_Count'] = 0\n",
    "    \n",
    "    # 客室番号の偶数奇数\n",
    "    df['Cabin_Number_Odd'] = (df['Cabin_Number'] % 2).fillna(0)\n",
    "    \n",
    "    # ========== 4. 基本的な前処理（exp001と同様） ==========\n",
    "    # 性別\n",
    "    df['Sex'] = df['Sex'].map({'female': 0, 'male': 1})\n",
    "    \n",
    "    # 年齢の欠損値補完（性別と客室クラス別の中央値）\n",
    "    df['Age'] = df.groupby(['Sex', 'Pclass'])['Age'].transform(lambda x: x.fillna(x.median()))\n",
    "    \n",
    "    # 運賃の欠損値補完（客室クラス別の中央値）\n",
    "    df['Fare'] = df.groupby('Pclass')['Fare'].transform(lambda x: x.fillna(x.median()))\n",
    "    \n",
    "    # 乗船港の欠損値補完（最頻値）\n",
    "    df['Embarked'] = df['Embarked'].fillna('S')\n",
    "    \n",
    "    # ========== 5. 家族構成の高度な特徴量 ==========\n",
    "    # 家族サイズ\n",
    "    df['FamilySize'] = df['SibSp'] + df['Parch'] + 1\n",
    "    \n",
    "    # 詳細な家族構成\n",
    "    df['IsAlone'] = (df['FamilySize'] == 1).astype(int)\n",
    "    df['IsSmallFamily'] = ((df['FamilySize'] >= 2) & (df['FamilySize'] <= 4)).astype(int)\n",
    "    df['IsLargeFamily'] = (df['FamilySize'] > 4).astype(int)\n",
    "    \n",
    "    # 特定の家族関係\n",
    "    df['HasSpouse'] = (df['SibSp'] > 0).astype(int)\n",
    "    df['HasChildren'] = (df['Parch'] > 0).astype(int)\n",
    "    df['HasParents'] = (df['Parch'] > 0).astype(int)\n",
    "    df['HasSiblings'] = (df['SibSp'] > 0).astype(int)\n",
    "    \n",
    "    # 家族関係の組み合わせ\n",
    "    df['Spouse_Children'] = df['HasSpouse'] * df['HasChildren']\n",
    "    df['ParentsChildren_Alone'] = (1 - df['HasSpouse']) * df['HasChildren']\n",
    "    \n",
    "    # ========== 6. 年齢・運賃の高度な処理 ==========\n",
    "    # 年齢のビニング（より詳細）\n",
    "    df['Age_Group'] = pd.cut(df['Age'], \n",
    "                           bins=[0, 12, 18, 25, 35, 50, 65, 100], \n",
    "                           labels=['Child', 'Teen', 'Young', 'Adult', 'Middle', 'Senior', 'Elder'])\n",
    "    \n",
    "    # 運賃のビニング（より詳細）\n",
    "    df['Fare_Group'] = pd.qcut(df['Fare'], q=8, labels=['Fare1', 'Fare2', 'Fare3', 'Fare4', \n",
    "                                                       'Fare5', 'Fare6', 'Fare7', 'Fare8'])\n",
    "    \n",
    "    # 年齢の非線形変換\n",
    "    df['Age_Log'] = np.log1p(df['Age'])\n",
    "    df['Age_Squared'] = df['Age'] ** 2\n",
    "    \n",
    "    # 運賃の非線形変換\n",
    "    df['Fare_Log'] = np.log1p(df['Fare'])\n",
    "    df['Fare_Sqrt'] = np.sqrt(df['Fare'])\n",
    "    \n",
    "    # ========== 7. 交互作用特徴量 ==========\n",
    "    # 重要な交互作用（exp001で効果的だったもの）\n",
    "    df['Sex_Pclass'] = df['Sex'] * df['Pclass']\n",
    "    \n",
    "    # 新しい交互作用\n",
    "    df['Age_Fare_Interaction'] = df['Age'] * df['Fare']\n",
    "    df['Age_FamilySize'] = df['Age'] * df['FamilySize']\n",
    "    df['Fare_FamilySize'] = df['Fare'] * df['FamilySize']\n",
    "    df['Pclass_FamilySize'] = df['Pclass'] * df['FamilySize']\n",
    "    \n",
    "    # ========== 8. 統計的特徴量 ==========\n",
    "    # 年齢の相対位置（同じ性別・クラス内での順位）\n",
    "    df['Age_Rank_SexPclass'] = df.groupby(['Sex', 'Pclass'])['Age'].rank(pct=True)\n",
    "    \n",
    "    # 運賃の相対位置（同じクラス内での順位）\n",
    "    df['Fare_Rank_Pclass'] = df.groupby('Pclass')['Fare'].rank(pct=True)\n",
    "    \n",
    "    # 家族内での年齢順位（同じ苗字内）\n",
    "    df['Age_Rank_Family'] = df.groupby('Surname')['Age'].rank(pct=True)\n",
    "    \n",
    "    return df\n",
    "\n",
    "# 特徴量エンジニアリング実行\n",
    "train_processed = advanced_feature_engineering(train_df, is_train=True)\n",
    "test_processed = advanced_feature_engineering(test_df, is_train=False)\n",
    "\n",
    "print(f\"特徴量エンジニアリング後 - Train shape: {train_processed.shape}\")\n",
    "print(f\"特徴量エンジニアリング後 - Test shape: {test_processed.shape}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## カテゴリカル特徴量のエンコーディング"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# カテゴリカル特徴量のエンコーディング\n",
    "categorical_features = [\n",
    "    'Embarked', 'Title_Grouped', 'Cabin_Deck', 'Ticket_Prefix',\n",
    "    'Age_Group', 'Fare_Group'\n",
    "]\n",
    "\n",
    "# Label Encoding\n",
    "label_encoders = {}\n",
    "for feature in categorical_features:\n",
    "    if feature in train_processed.columns:\n",
    "        le = LabelEncoder()\n",
    "        # train+testの全カテゴリでfitして一貫性を保つ\n",
    "        all_categories = pd.concat([train_processed[feature], test_processed[feature]]).astype(str)\n",
    "        le.fit(all_categories)\n",
    "        \n",
    "        train_processed[feature] = le.transform(train_processed[feature].astype(str))\n",
    "        test_processed[feature] = le.transform(test_processed[feature].astype(str))\n",
    "        \n",
    "        label_encoders[feature] = le\n",
    "        print(f\"{feature}: {len(le.classes_)} categories\")\n",
    "\n",
    "print(\"\\nエンコーディング完了\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## 特徴量選択"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# 学習用特徴量の選択\n",
    "exclude_features = ['PassengerId', 'Name', 'Ticket', 'Cabin', 'Survived', 'Surname', 'Title']\n",
    "feature_columns = [col for col in train_processed.columns if col not in exclude_features]\n",
    "\n",
    "# 数値でない列を除外\n",
    "numeric_features = []\n",
    "for col in feature_columns:\n",
    "    if train_processed[col].dtype in ['int64', 'float64', 'int32', 'float32']:\n",
    "        numeric_features.append(col)\n",
    "    else:\n",
    "        print(f\"非数値特徴量を除外: {col} (dtype: {train_processed[col].dtype})\")\n",
    "\n",
    "feature_columns = numeric_features\n",
    "\n",
    "print(f\"\\n使用する特徴量数: {len(feature_columns)}\")\n",
    "print(\"特徴量一覧:\")\n",
    "for i, col in enumerate(feature_columns, 1):\n",
    "    print(f\"{i:2d}. {col}\")\n",
    "\n",
    "# 訓練データとテストデータの準備\n",
    "X = train_processed[feature_columns]\n",
    "y = train_processed['Survived']\n",
    "X_test = test_processed[feature_columns]\n",
    "\n",
    "print(f\"\\nX shape: {X.shape}\")\n",
    "print(f\"y shape: {y.shape}\")\n",
    "print(f\"X_test shape: {X_test.shape}\")"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "## モデル訓練と評価"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# LightGBMパラメータ\n",
    "lgb_params = {\n",
    "    'objective': 'regression',\n",
    "    'metric': 'rmse',\n",
    "    'boosting_type': 'gbdt',\n",
    "    'num_leaves': 31,\n",
    "    'learning_rate': 0.1,\n",
    "    'feature_fraction': 0.8,\n",
    "    'bagging_fraction': 0.8,\n",
    "    'bagging_freq': 5,\n",
    "    'verbose': -1,\n",
    "    'random_state': 42\n",
    "}\n",
    "\n",
    "# 交差検証\n",
    "n_folds = 5\n",
    "kf = StratifiedKFold(n_splits=n_folds, shuffle=True, random_state=42)\n",
    "\n",
    "cv_scores = []\n",
    "feature_importance = pd.DataFrame()\n",
    "oof_predictions = np.zeros(len(X))\n",
    "test_predictions = np.zeros(len(X_test))\n",
    "\n",
    "print(\"=== 交差検証開始 ===\")\n",
    "\n",
    "for fold, (train_idx, val_idx) in enumerate(kf.split(X, y), 1):\n",
    "    print(f\"\\nFold {fold}/{n_folds}\")\n",
    "    \n",
    "    # データ分割\n",
    "    X_train, X_val = X.iloc[train_idx], X.iloc[val_idx]\n",
    "    y_train, y_val = y.iloc[train_idx], y.iloc[val_idx]\n",
    "    \n",
    "    # LightGBMデータセット作成\n",
    "    train_data = lgb.Dataset(X_train, label=y_train)\n",
    "    val_data = lgb.Dataset(X_val, label=y_val, reference=train_data)\n",
    "    \n",
    "    # モデル訓練\n",
    "    model = lgb.train(\n",
    "        lgb_params,\n",
    "        train_data,\n",
    "        valid_sets=[val_data],\n",
    "        num_boost_round=1000,\n",
    "        callbacks=[lgb.early_stopping(100), lgb.log_evaluation(0)]\n",
    "    )\n",
    "    \n",
    "    # 予測\n",
    "    val_pred = model.predict(X_val)\n",
    "    test_pred = model.predict(X_test)\n",
    "    \n",
    "    # 二値分類のため0.5で閾値処理\n",
    "    val_pred_binary = (val_pred >= 0.5).astype(int)\n",
    "    \n",
    "    # 精度計算\n",
    "    fold_score = accuracy_score(y_val, val_pred_binary)\n",
    "    cv_scores.append(fold_score)\n",
    "    \n",
    "    print(f\"Fold {fold} Accuracy: {fold_score:.4f}\")\n",
    "    \n",
    "    # OOF予測保存\n",
    "    oof_predictions[val_idx] = val_pred\n",
    "    test_predictions += test_pred / n_folds\n",
    "    \n",
    "    # 特徴量重要度\n",
    "    fold_importance = pd.DataFrame()\n",
    "    fold_importance['feature'] = feature_columns\n",
    "    fold_importance['importance'] = model.feature_importance()\n",
    "    fold_importance['fold'] = fold\n",
    "    feature_importance = pd.concat([feature_importance, fold_importance])\n",
    "\n",
    "# CV結果\n",
    "mean_cv_score = np.mean(cv_scores)\n",
    "std_cv_score = np.std(cv_scores)\n",
    "\n",
    "print(f\"\\n=== CV結果 ===\")\n",
    "print(f\"CV Accuracy: {mean_cv_score:.4f} ± {std_cv_score:.4f}\")\n",
    "\n",
    "# OOF精度\n",
    "oof_binary = (oof_predictions >= 0.5).astype(int)\n",
    "oof_accuracy = accuracy_score(y, oof_binary)\n",
    "print(f\"OOF Accuracy: {oof_accuracy:.4f}\")"
   ]
  },
  {
   "cell_type": "markdown",
